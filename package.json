{
  "name": "wtorrent-app",
  "version": "0.0.1",
  "description": "Dashboard for webTorrent - Chrome App",
  "main": "server.js",
  "scripts": {
    "postinstall": "browserinc",
    "build": "browserify lib/index.js -g babelify -g aliasify -g declareify --outfile public/index.js -v && crx pack ./public -p ~/.ssh/wtorrent.pem -o ./extension.crx",
    "watch": "crxify -p ~/.ssh/wtorrent.pem & watchify lib/index.js -g babelify -g aliasify -g declareify --outfile public/index.js -v"
  },
  "author": "Aram Drevekenin",
  "license": "MIT",
  "dependencies": {
    "aliasify": "^1.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserinc": "^1.1.0",
    "chrome-debug": "^1.0.0",
    "chrome-dgram": "^2.1.7",
    "chrome-net": "^3.1.1",
    "chrome-path": "^1.0.0",
    "chrome-util-deprecate": "^1.0.0",
    "crx": "^3.0.3",
    "declareify": "^2.0.0",
    "parse-torrent": "^5.7.0",
    "redux": "^3.3.1",
    "watchify": "^3.1.1",
    "web-fs": "^2.1.0",
    "webtorrent": "^0.72.1"
  },
  "aliasify": {
    "aliases": {
      "net": "chrome-net",
      "dgram": "chrome-dgram",
      "dns": "native-dns",
      "fs": "./public/fs-wrapper.js",
      "addr-to-ip-port": "addr-to-ip-port/index",
      "load-ip-set": "load-ip-set/index",
      "ut_pex": "ut_pex/index",
      "simple-get": "simple-get/index",
      "debug": "chrome-debug",
      "depd": "chrome-depd",
      "path": "chrome-path",
      "util-deprecate": "chrome-util-deprecate"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": [
      "cuid"
    ]
  },
  "browserinc": {
    "include": [
      "./lib/common-node.js",
      "./lib/client/http-tracker.js",
      "./lib/client/udp-tracker.js",
      "./lib/server.js",
      "fs-chunk-store"
    ]
  }
}
